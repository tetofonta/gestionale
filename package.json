  "private": true,
  "proxy": "https://localhost:5000/",
  "dependencies": {
    "webpack": "~4.19.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "@material-ui/core": "^3.6.1",
