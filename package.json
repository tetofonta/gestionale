    "body-parser": "^1.18.3",
    "chart.js": "^2.7.3",
    "contentful": "^7.0.3",
    "cors": "^2.8.5",
    "downshift": "^2.2.2",
    "express": "^4.16.3",
    "flatten": "^1.0.2",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "jspdf": "^1.4.1",
    "mosca": "^2.8.3",
    "mqtt": "^2.18.8",
    "mysql": "^2.16.0",
    "os-utils": "^0.0.14",
    "prop-types": "latest",
    "qrcode.react": "^0.8.0",
    "qrious": "^4.0.2",
    "react": "^16.5.2",
    "react-chartjs2": "^1.2.1",
    "react-dom": "^16.5.2",
    "react-qr-reader": "^2.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "request": "^2.88.0",
    "secure-random-string": "^1.1.0",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "pkg": "^4.3.5",
    "rewire": "^4.0.1",
    "webpack": "~4.19.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start-server": "cd backend && node init.js",
    "start-manager": "cd backend/dataManager && node manager.js",
    "start-broker": "cd backend/MQTT && node broker.js",
    "start-order-printer": "cd backend/MQTT/CLIENTS/orderPrinter && node orderPrinter.js",
    "start-order-processor": "cd backend/MQTT/CLIENTS/orderProcessor && node orderProcessor.js",
    "start-guest-tracker": "cd backend/MQTT/CLIENTS/guestTracker && node guestTracker.js",
    "start-captive": "cd backend/CAPTIVE && node captive.js",
    "start-load-balancer": "cd backend/LOAD_BALANCER && node lb.js",
    "start": "HTTPS=true && react-scripts start",
    "dev_ws_essential": "yarn concurrently --kill-others-on-fail \"yarn start-server\" \"yarn start-manager\" \"yarn start-broker\" \"yarn start-guest-tracker -worker-enable\" \"yarn start\"",
    "dev_ws": "yarn concurrently --kill-others-on-fail \"yarn start-server\" \"yarn start-manager\" \"yarn start-broker\" \"yarn start-guest-tracker -worker-enable\" \"yarn start-order-processor\" \"yarn start-order-printer\" \"yarn start\"",
    "dev_load_balancer": "yarn concurrently --kill-others-on-fail \"yarn start-load-balancer\" \"yarn start-server\" \"yarn start-manager\" \"yarn start-broker\" \"yarn start-guest-tracker -worker-enable\" \"yarn start-order-processor\" \"yarn start-order-printer\" \"yarn start\"",
    "dev": "yarn concurrently --kill-others-on-fail \"yarn start-load-balancer\" \"yarn start-captive\" \"yarn start-server\" \"yarn start-manager\" \"yarn start-broker\" \"yarn start-guest-tracker -worker-enable\" \"yarn start-order-processor\" \"yarn start-order-printer\" \"yarn start\"",
    "build": "react-scripts build",
    "deploy": "bash deploy.bash",
    "test": "yarn mocha TEST/"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pkg": {
    "scripts": "backend/modules/built/*.js"
  }
}
