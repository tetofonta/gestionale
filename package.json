    "babel-loader": "^8.0.4",
    "base-64": "^0.1.0",
    "body-parser": "^1.18.3",
    "contentful": "^7.0.3",
    "downshift": "^2.2.2",
    "express": "^4.16.3",
    "flatten": "^1.0.2",
    "global": "^4.3.2",
    "jspdf": "^1.4.1",
    "mqtt": "^2.18.8",
    "mysql": "^2.16.0",
    "qrcode.react": "^0.8.0",
    "qrious": "^4.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-qr-reader": "^2.1.1",
    "chart.js": "^2.7.3",
    "react-chartjs2": "^1.2.1",
    "secure-random-string": "^1.1.0",
    "cors": "^2.8.5",
    "mosca": "^2.8.3",
    "history": "^4.7.2",
    "prop-types": "latest",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "pkg": "^4.3.5",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "rewire": "^4.0.1",
    "istanbul": "^0.4.5"
  },
  "scripts": {
    "start-server": "cd backend && node init.js",
    "start-mqtt": "cd backend/MQTT && node broker.js",
    "start-order-printer": "cd backend/MQTT/CLIENTS/orderPrinter && node orderPrinter.js",
    "start-order-processor": "cd backend/MQTT/CLIENTS/orderProcessor && node orderProcessor.js",
    "start-guest-tracker": "cd backend/MQTT/CLIENTS/guestTracker && node guestTracker.js",
    "start-guest-tracker-worker": "cd backend/MQTT/CLIENTS/guestTracker && node guestTracker.js -worker-enable",
    "start-stats-tracker": "cd backend/MQTT/CLIENTS/statsLogger && node statsLogger.js",
    "start": "HTTPS=true && react-scripts start",
    "dev": "yarn concurrently --kill-others-on-fail \"yarn start-server\" \"yarn start\" \"yarn start-mqtt\" \"yarn start-order-printer\" \"yarn start-order-processor\" \"yarn start-guest-tracker\" \"yarn start-guest-tracker-worker\" \" yarn start-stats-tracker\"",
    "build": "react-scripts build",
    "deploy": "sh deploy.sh",
    "test": "yarn mocha TEST/maketest.js"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
