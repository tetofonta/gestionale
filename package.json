{
  "name": "vezo",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:5000/",
  "dependencies": {
    "babel-loader": "8.0.4",
    "contentful": "^7.0.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^2.1.1",
    "react-router": "~4.3.1",
    "react-router-dom": "~4.3.1",
    "react-pdf": "^3.0.5",
    "@material-ui/core": "~3.1.0",
    "@material-ui/icons": "~3.0.1",
    "downshift": "~2.2.2",
    "express": "~4.16.3",
    "body-parser": "~1.18.3",
    "secure-random-string": "^1.1.0",
    "mysql": "~2.16.0",
    "mqtt": "~2.18.8",
    "qrcode.react": "~0.8.0",
    "qrious": "^4.0.2",
    "webpack-hot-middleware": "^2.24.3",
    "jspdf": "^1.4.1",
    "base-64": "~0.1.0",
    "mosca": "^2.8.3",
    "flatten": "^1.0.2"
  },
  "scripts": {
    "start-server": "cd backend && nodemon init.js",
    "start-mqtt": "cd backend/MQTT && nodemon broker.js",
    "start-order-printer": "cd backend/MQTT/CLIENTS/orderPrinter && nodemon orderPrinter.js",
    "start-order-processor": "cd backend/MQTT/CLIENTS/orderProcessor && nodemon orderProcessor.js",
    "start": "HTTPS=true && react-scripts start",
    "dev": "concurrently --kill-others-on-fail \"yarn start-server\" \"yarn start\" \"yarn start-mqtt\" \"yarn start-order-printer\" \"yarn start-order-processor\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}